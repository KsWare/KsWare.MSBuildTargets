<?xml version="1.0" encoding="utf-8" ?>
<package >
  <!--
	Any nuget reference you add will be included in the nuget package as a dependency without you
	having to add it in here.
	
	For more info on how to compose a nuspec file see http://docs.nuget.org/create/nuspec-reference 
	-->

  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>$id$</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <projectUrl>https://github.com/$author$/$id$</projectUrl>
    <!-- LICENSE see https://docs.microsoft.com/en-US/nuget/reference/nuspec#license  -->
    <!-- deprecated <licenseUrl>https://github.com/$author$/$id$/blob/master/LICENSE</licenseUrl> -->
    <!-- <license type="expression">MIT</license> -->
    <license type="file">LICENSE</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://raw.githubusercontent.com/$author$/$id$/master/icon.png</iconUrl>
    <description>$description$</description>
    <developmentDependency>false</developmentDependency>
    <releaseNotes>https://raw.githubusercontent.com/$author$/$id$/master/ReleaseNotes.md</releaseNotes>
    <copyright>Copyright 2019 by $author$. All rights reserved.</copyright>
    <tags>$author$</tags>
  </metadata>
  <files>
    <file src="LICENSE*" target="" />
  </files>
</package>